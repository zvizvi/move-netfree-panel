{
	"update_url": "https://clients2.google.com/service/update2/crx",

	"name": "Move Netfree panel",
	"version": "0.2.5",
	"author": "zvizvi5@outlook.com",
	"permissions": [
		"http://*/*",
		"https://*/*",
		"storage"
	],
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
			"matches": [ "http://*/*", "https://*/*" ],
			"js": [ "js/jquery-1.11.3.min.js" , "js/main.js" ]
		}
	],
	"manifest_version":2
}
