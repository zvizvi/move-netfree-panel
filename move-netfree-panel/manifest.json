{
	"name": "Move Netfree panel",
	"version": "0.2.5",
	"author": "zvizvi5@outlook.com",
	"permissions": [
		"tabs",
		"http://*/*",
		"https://*/*"
	],
	"browser_action": {
		"default_icon": "icon.png"
	},
	"content_scripts": [
		{
			"matches": [ "http://*/*", "https://*/*" ],
			"js": [ "js/jquery-1.11.3.min.js" , "js/main.js" ]
		} 
	],
	"manifest_version":2
}
